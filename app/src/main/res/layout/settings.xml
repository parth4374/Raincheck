        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Precipitation Low"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="45dp" />

    <TextView
        android:id="@+id/textView4"
